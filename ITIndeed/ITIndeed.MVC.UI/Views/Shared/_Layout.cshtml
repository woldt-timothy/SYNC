<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - SYNC</title>
    @*@Styles.Render("~/Content/Theme/css")
        @Scripts.Render("~/bundles/modernizr")*@
    <link href="/Content/Theme/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/Theme/css/font-awesome.min.css" rel="stylesheet" />
    <link href="/Content/Theme/css/animate.css" rel="stylesheet" />
    <link href="/Content/Theme/css/style.css" rel="stylesheet" />

    <script src="/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    @RenderBody()
    <header>
        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse.collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <div class="navbar-brand">
                            <a href="../../Content/Theme/index.html"><img src="../../Content/Theme/images/logo.png" class="img-responsive" style="width:7em;"></a>
                        </div>
                    </div>

                    <div class="navbar-collapse collapse">
                        <div class="menu">
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation"><a href="../../Content/Theme/index.html">Home</a></li>
                                <li role="presentation"><a href="../../Content/Theme/about.html">About Us</a></li>
                                <li role="presentation"><a href="../../Content/Theme/portfolio.html">Portfolio</a></li>
                                <li role="presentation"><a href="../../Content/Theme/blog.html">Blog</a></li>
                                <li role="presentation"><a href="../../Content/Theme/contact.html">Contact</a></li>
                                <li role="presentation">@Html.ActionLink("Employer Search", "Index", "EmployerProfile")</li>
                                <li role="presentation">@Html.ActionLink("Login", "Login", "Login")</li>
                                <li role="presentation">@Html.ActionLink("Logout", "Logout", "Login")</li>
                                <li role="presentation">@Html.ActionLink("Events", "Index", "Event")</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </header>




    @*<section id="conatcat-info">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8">
                        <div class="media contact-info wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                            <div class="pull-left">
                                <i class="fa fa-phone"></i>
                            </div>
                            <div class="media-body">
                                <h2>Have a question or need assistance?</h2>
                                <p>Call our customer support number (1800)123-4567 or contact us via email, syncsupport@sync.com</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/.container-->
        </section>
        <!--/#conatcat-info-->*@

    <footer>
        <div class="footer navbar-fixed-bottom">
            <div class="container">
                <div class="social-icon">
                    <div class="col-md-4">
                        <ul class="social-network">
                            <li><a href="#" class="fb tool-tip" title="Facebook"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#" class="twitter tool-tip" title="Twitter"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#" class="gplus tool-tip" title="Google Plus"><i class="fa fa-google-plus"></i></a></li>
                            <li><a href="#" class="linkedin tool-tip" title="Linkedin"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#" class="ytube tool-tip" title="You Tube"><i class="fa fa-youtube-play"></i></a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-md-4 col-md-offset-4">
                    <div class="copyright">
                        &copy; Sync. All Rights Reserved.
                    </div>
                </div>
            </div>

            <div class="pull-right">
                <a href="#home" class="scrollup"><i class="fa fa-angle-up fa-3x"></i></a>
            </div>
        </div>
    </footer>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/Content/Theme/js")*@
    <script src="/Scripts/jquery-3.3.1.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
